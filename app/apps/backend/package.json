{
  "name": "@fans/backend",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "build": "nest build",
    "migrate:dev": "pnpm run build && pnpx typeorm migration:run -d ./dist/config.js",
    "migrate:prod": "pnpx typeorm migration:run -d ./dist/config.js",
    "start:dev": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test:prod": "jest",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "10.3.0",
    "@nestjs/core": "10.3.0"
  },
  "devDependencies": {
    "@fans/types": "workspace:^",
    "@nestjs/bull": "10.0.1",
    "@nestjs/cache-manager": "2.1.1",
    "@nestjs/cli": "10.2.1",
    "@nestjs/config": "3.1.1",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/platform-fastify": "10.3.0",
    "@nestjs/schematics": "10.0.3",
    "@nestjs/testing": "10.3.0",
    "@nestjs/typeorm": "10.0.1",
    "@types/cache-manager-redis-store": "2.0.4",
    "@types/node-telegram-bot-api": "0.64.1",
    "axios": "1.6.3",
    "bull": "4.12.0",
    "cache-manager": "5.3.2",
    "cache-manager-redis-store": "3.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "jimp": "^0.22.10",
    "node-telegram-bot-api": "0.64.0",
    "pg": "8.11.3",
    "pino": "8.17.2",
    "pino-pretty": "10.3.1",
    "redis": "4.6.12",
    "ts-loader": "9.5.1",
    "typeorm": "0.3.17"
  }
}
