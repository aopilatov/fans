{
  "name": "@fans/site",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/site",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "dependsOn": [{ "projects": "dependencies", "target": "build" }],
      "options": {
        "outputPath": "apps/site/.next",
        "main": "apps/site/src/main.ts",
        "tsConfig": "apps/site/tsconfig.build.json",
        "assets": ["apps/site/*.md"]
      }
    }
  }
}
